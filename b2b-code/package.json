{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit false && vite build",
    "vercel-build": "npm run build",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "npm run build:server && node dist-server/server/index.js",
    "dev:server": "npx nodemon --ext ts,js --watch server --watch src/config --watch src/models --watch src/services --watch src/lib --exec \"npm run build:server && node dist-server/server/index.js\"",
    "debug:server": "tsc -p tsconfig.server.json && node --inspect dist-server/server/index.js",
    "start:dev": "concurrently \"npm run dev\" \"npm run dev:server\""
  },
  "dependencies": {
    "@babel/runtime": "^7.27.1",
    "@headlessui/react": "^1.7.18",
    "@mistralai/mistralai": "^1.6.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/axios": "^0.14.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.3",
    "framer-motion": "^11.0.8",
    "i18next": "^23.10.1",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.3",
    "react": "^18.3.1",
    "react-calendar": "^4.8.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.0",
    "react-i18next": "^14.1.0",
    "react-pdf": "^9.2.1",
    "react-router-dom": "^6.22.2",
    "react-speech-recognition": "^4.0.0",
    "regenerator-runtime": "^0.14.1",
    "tailwind-merge": "^3.1.0",
    "three": "^0.162.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/next": "^9.0.0",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/webxr": "^0.5.22",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.19"
  }
}
